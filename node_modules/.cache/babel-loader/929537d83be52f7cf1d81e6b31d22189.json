{"ast":null,"code":"import Image from './components/Image.vue';\nimport Heading from './components/Heading.vue';\nimport Content from './components/Content.vue';\nimport ProdCard from './components/ProdCard.vue';\nexport default {\n  name: 'App',\n  components: {\n    Image: Image,\n    Heading: Heading,\n    Content: Content,\n    ProdCard: ProdCard\n  },\n  data: function data() {\n    return {\n      fullName: 'Dolhova Yelyzaveta Olehivna',\n      dateOfBirth: 'Date of birth: 03.04.2002, Donetsk, Ukraine',\n      education: 'Graduated lyceum \"Intellect\", 4th year student in KPI',\n      hobbies: ['Crochet', 'Art makeup', 'Cycling'],\n      books: ['American tragedy - Theodore Dreiser', 'Plague - Albert Camus', '1984 - George Orwell'],\n      kyivDescription: \"Out of all cities I've been to Kyiv is still the best and the most beautiful one!\"\n    };\n  },\n  computed: {\n    products: function products() {\n      return this.$store.state.products;\n    }\n  },\n  created: function created() {\n    this.$store.dispatch('fetchProducts');\n  }\n};","map":{"version":3,"mappings":"AAkDE,OAAOA,KAAP,MAAkB,wBAAlB;AACA,OAAOC,OAAP,MAAoB,0BAApB;AACA,OAAOC,OAAP,MAAoB,0BAApB;AACA,OAAOC,QAAP,MAAqB,2BAArB;AAEA,eAAe;EACbC,IAAI,EAAE,KADO;EAEbC,UAAU,EAAE;IAAEL,KAAK,EAALA,KAAF;IAASC,OAAO,EAAPA,OAAT;IAAkBC,OAAO,EAAPA,OAAlB;IAA2BC,QAAO,EAAPA;EAA3B,CAFC;EAGbG,IAHa,kBAGN;IACL,OAAO;MACLC,QAAQ,EAAE,6BADL;MAELC,WAAW,EAAE,6CAFR;MAGLC,SAAS,EAAE,uDAHN;MAILC,OAAO,EAAE,CAAC,SAAD,EAAY,YAAZ,EAA0B,SAA1B,CAJJ;MAKLC,KAAK,EAAE,CACL,qCADK,EAEL,uBAFK,EAGL,sBAHK,CALF;MAULC,eAAe,EACb;IAXG,CAAP;EAaD,CAjBY;EAkBbC,QAAQ,EAAE;IACRC,QADQ,sBACG;MACT,OAAO,KAAKC,MAAL,CAAYC,KAAZ,CAAkBF,QAAzB;IACD;EAHO,CAlBG;EAuBbG,OAvBa,qBAuBH;IACR,KAAKF,MAAL,CAAYG,QAAZ,CAAqB,eAArB;EACD;AAzBY,CAAf","names":["Image","Heading","Content","ProdCard","name","components","data","fullName","dateOfBirth","education","hobbies","books","kyivDescription","computed","products","$store","state","created","dispatch"],"sourceRoot":"","sources":["/Users/liza/Documents/uni/web-dev-labs-lab7/src/App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <head>\n      <title>Dolhova Yelyzaveta, IP-94, Lab7, Variant 2</title>\n      <meta charset=\"UTF-8\" />\n      <meta\n        name=\"keywords\"\n        content=\"Dolhova, IP-94, Yelyzaveta\"\n      />\n      <meta\n        name=\"description\"\n        content=\"Laboratory work 7 by Dolhova\"\n      />\n    </head>\n    <body>\n      <Heading :text=\"fullName\" />\n      <Content\n        :data=\"[dateOfBirth, education]\"\n        type=\"paragraph\"\n      />\n\n      <Heading text=\"Hobbies\" />\n      <Content\n        :data=\"hobbies\"\n        type=\"ul\"\n      />\n\n      <Heading text=\"Favourite books\" />\n      <Content\n        :data=\"books\"\n        type=\"ol\"\n      />\n\n      <Content\n        :data=\"[kyivDescription]\"\n        type=\"paragraph\"\n      />\n      <Image />\n\n      <div id=\"container\">\n        <ProdCard\n          v-for=\"product in products\"\n          :product=\"product\"\n        />\n      </div>\n    </body>\n  </div>\n</template>\n\n<script>\n  import Image from './components/Image.vue';\n  import Heading from './components/Heading.vue';\n  import Content from './components/Content.vue';\n  import ProdCard from './components/ProdCard.vue';\n\n  export default {\n    name: 'App',\n    components: { Image, Heading, Content, ProdCard },\n    data() {\n      return {\n        fullName: 'Dolhova Yelyzaveta Olehivna',\n        dateOfBirth: 'Date of birth: 03.04.2002, Donetsk, Ukraine',\n        education: 'Graduated lyceum \"Intellect\", 4th year student in KPI',\n        hobbies: ['Crochet', 'Art makeup', 'Cycling'],\n        books: [\n          'American tragedy - Theodore Dreiser',\n          'Plague - Albert Camus',\n          '1984 - George Orwell',\n        ],\n        kyivDescription:\n          \"Out of all cities I've been to Kyiv is still the best and the most beautiful one!\",\n      };\n    },\n    computed: {\n      products() {\n        return this.$store.state.products;\n      },\n    },\n    created() {\n      this.$store.dispatch('fetchProducts');\n    },\n  };\n</script>\n\n<style>\n  #container {\n    display: flex;\n    flex-direction: row;\n    margin-top: 2rem;\n    flex-wrap: wrap;\n    justify-content: center;\n  }\n</style>\n"]},"metadata":{},"sourceType":"module"}